@article{wolkomirfutreal_1997b,
	abstract = {The article examines the debate on whether Christian
		  dominion doctrine fosters negative environmental attitudes
		  and outcomes. Sociologist Lynn White's now oft-cited and
		  controversial essay, "The Historical Roots of Our Ecologic
		  Crisis" contends that people shall continue to have a
		  worsening ecologic crisis until they reject the Christian
		  axiom that nature has no reason for existence, save to
		  serve man. This contention has come to be known by some
		  sociologists as the White Thesis, the assertion that
		  Judeo-Christian doctrine, specifically the Genesis account
		  of creation, encourages a dominion belief that justifies
		  environmental explanation. A growing body of research
		  explores connections between religion and environmental
		  attitudes or behaviors. In an early contribution, some
		  researchers analyzed the relationship of religious
		  preference and commitment, mastery-over-nature orientation,
		  and individuals' environmental concern. The results provide
		  support for the White Thesis in that, Judeo-Christians are
		  more likely than others to advocate mastery-over-nature and
		  tend to have lower levels of environmental concern.},
	author = {Wolkomir, Michelle and Futreal, Michael and Woodrum, Eric and Hoban, Thomas},
	citation_identifier = {Wolkomir 2097},
	date-added = {2015-11-06 12:38:55 +0000},
	date-modified = {2016-02-17 09:17:48 +0000},
