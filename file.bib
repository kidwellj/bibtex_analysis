	journal = {npj Climate Action},
	number = {1},
	pages = {78},
	title = {Belief in divine (versus human) control of earth affects perceived threat of climate change},
	volume = {3},
	year = {2024},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://doi.org/10.1038/s44168-024-00163-9}}

