	month = {3},
	number = {1},
	pages = {198-215},
	title = {Fracturing Over Creation Care? Shifting Environmental Beliefs Among Evangelicals, 1984-2010},
	uuid = {9F6C635F-5021-470D-B83C-6051A1988F58},
	volume = {52},
	year = {2013},
