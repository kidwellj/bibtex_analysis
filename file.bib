	number = {4},
	pages = {387-404},
	publisher = {Religious Research Association, Inc.},
	title = {The Sanctification of Nature and Theological Conservatism: A Study of Opposing Religious Correlates of Environmentalism},
	url = {http://www.jstor.org/stable/3512131},
