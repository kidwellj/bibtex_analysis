@article{ilokambakwem_2022a,
	abstract = {The earth's climate has been changing for generations but in recent times,
drastic weather changes are coming on faster than it did in the past. Drastic
weather patterns and changes are leading to unpredictable and increased natural
hazards. There is limited research available which try to understand how
religious beliefs could influence perceptions on climate change in Nigeria.
Religion and faith have always played vital roles in the lives of people trying to
deal with disasters which occur from natural hazards. This research tries to understand how religious belief shape views on climatic hazards, especially in
developing communities' context. Findings from this research suggest that,
while some people in local communities believe that climatic hazards serve as
warnings from their gods to change from their evil ways, some other people
show some degree of understanding about changes in the earth's climate. They
deduce that increased rainfall intensity and changes in seasonal patterns
essential for crop production is increasing vulnerability to natural hazards in
their communities.},
	author = {Nnamdi Greg Iloka and Enyinnaya Mbakwem},
	date-added = {2025-03-17 17:24:25 +0000},
	date-modified = {2025-03-17 17:25:41 +0000},
	doi = {10.31920/2634-7644/2022/v3n1a4},
	journal = {African Journal of Religion, Philosophy and Culture},
	number = {1},
	pages = {61-80},
	title = {Religious Notions and Climatic Hazards: Perceptions in Nsukka Communities of Enugu State, Nigeria},
	volume = {3},
	year = {2022},
	bdsk-file-1 = {}}

