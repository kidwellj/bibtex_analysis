	number = {2},
	pages = {373-391},
	rating = {5},
	title = {An Examination of the "Greening of Christianity" Thesis Among Americans, 1993-2010},
	uuid = {6FD7DF12-0EFD-4359-8159-24CA86B104B6},
	volume = {53},
	year = {2014},
