@article{sherkatellison_2007a,
	abstract = {Recent research on the connection between religion and
		  environmental concern and activism has led to divergent
		  conclusions, with some studies finding a negative effect of
		  religious factors, and others finding no influence or a
		  positive effect. Using a conceptual apparatus of
