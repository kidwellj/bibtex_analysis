@mastersthesis{yatsko_2010a,
	abstract = {This study examines the mediating role that social values
		  may play in understanding generational cohort differences
		  in proenvironmental attitudes. Using the World Values
		  Survey and examining differences among Generation X (those
		  born between 1965 and 1981), Baby Boomers (those born
		  between 1946 and 1964), and the Silent Generation (those
		  born between 1921 and 1945), the results of the current
		  study suggest that proenvironmentalism operates as a
		  multidimensional construct that is not consistently related
		  to either cohort or social values. More specifically,
		  findings indicate that proenvironmental attitudes do not
		  differ as much as expected across the three generational
		  cohorts and that Generation X may not be as
		  proenvironmental as others have predicted. In addition, the
		  social values examined here (i.e., postmaterialism,
		  economic liberalism, and political interest) did not
		  explain the relationships between cohort and
		  proenvironmental attitudes.},
	author = {Yatsko, Loni Kay},
	citation_identifier = {Yatsko 2010},
