	month = {Dec.},
	number = {12},
	pages = {50--59},
	title = {Environmental Crisis and Religion: An Analysis of Textual Source and Practices of Sikhism},
	url = {https://rrjournals.com/index.php/rrijm/article/view/964},
	volume = {8},
	year = {2023},
