@article{delashmutt_2011a,
	abstract = {This article examines the extent to which the Church of
		  England's institutional environmental policies, practices,
		  and theologies are being translated into the context of the
		  parish church. This study uses a series of focus group
		  interviews to gather data from six parishes around the
		  Diocese of Truro to assess the attitudes towards, and
		  actions regarding, environment and climate change amongst
		  regular Church of England churchgoers. The study suggests
		  that despite a wealth of institutional resources that have
		  been developed to foster theologically informed
		  environmental knowledge, very little awareness of the
		  institutional Church's ethos is found in the local church
		  context.},
