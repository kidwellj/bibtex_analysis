	number = {8},
	pages = {704-718},
	publisher = {Taylor \& Francis},
	title = {Impacts of religion on environmental worldviews: The Teton Valley case},
	uuid = {6E11AE0D-2AE7-4051-966E-383A39060C5B},
	volume = {21},
	year = {2008},
