@article{woodrumdavison_1996a,
	abstract = {There is logical and social scientific reason to expect that individuals' theological orientations affect their dispositions toward the environment, just as it affects many other issues. That assumption underlies White (1967) and Toynbee's (1972) indictment of Judeo-Christianity as environmentally exploitative. A similar argument is advanced by some feminist theologians. They call for a characterization of God which is more feminine and accessible to enhance environmentalist causes. This prescription parallels arguments by Greeley (1992) who contends androgynous and nurturing images of God are most conducive to social and environmental concern. This study first uses 1993 general social survey data to assess the bases and extent of feminine egalitarian images of God. Next, we evaluate the significance of that image of God for environmental concern and behaviour. At present feminine, egalitarian images of God are uncommon in public. They are more prevalent among women and than men and among nonwhites than whites. Conventional church religion and personal religiosity are negatively associated with these nontraditional perceptions of God. Contrary to the presumptions of some authors, feminine egalitarian images of God are not statistically significant predictors of environmental concern. Of perhaps greater environmental significance is that persons who hold these images of God are significantly more active in positive environmental behavior. Trends in American religion make these theological options more available than in the past. They may have positive environmental significance.},
	author = {Woodrum, Eric and Davison, Beth L.},
	citation_identifier = {Woodrum 1996},
	date-added = {2015-11-06 12:38:55 +0000},
	date-modified = {2016-02-17 09:17:48 +0000},
