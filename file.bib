@article{armitageconner_2001a,
	abstract = {The Theory of Planned Behaviour (TPB) has received
		  considerable attention in the literature. The present study
		  is a quantitative integration and review of that research.
		  From a database of 185 independent studies published up to
		  the end of 1997, the TPB accounted for 27\% and 39\% of the
		  variance in behaviour and intention, respectively. The
		  perceived behavioural control (PBC) construct accounted for
		  significant amounts of variance in intention and behaviour,
		  independent of theory of reasoned action variables. When
		  behaviour measures were self-reports, the TPB accounted for
		  11\% more of the variance in behaviour than when behaviour
		  measures were objective or observed (R2s = .31 and .21,
		  respectively). Attitude, subjective norm and PBC account
		  for significantly more of the variance in individuals'
		  desires than intentions or self-predictions, but intentions
		  and self-predictions were better predictors of behaviour.
		  The subjective norm construct is generally found to be a
		  weak predictor of intentions. This is partly attributable
		  to a combination of poor measurement and the need for
		  expansion of the normative component. The discussion
		  focuses on ways in which current TPB research can be taken
		  forward in the light of the present review.},
	author = {Armitage, Christopher J. and Conner, Mark},
	citation_identifier = {Armitage 2001},
	date-added = {2015-11-06 12:38:55 +0000},
	date-modified = {2016-02-17 09:17:48 +0000},
	doi = {10.1348/014466601164939},
	journal = {British Journal of Social Psychology},
	keywords = {meta-analysis, bibdesk>empirical_research_R&E},
	month = {12},
	number = {4},
	pages = {471-499},
	title = {Efficacy of the Theory of Planned Behaviour: A meta-analytic review},
	uuid = {8835273B-0B89-4CDE-BD2F-401E7AF69908},
	volume = {40},
	year = {2001},
	bdsk-file-1 = {},
	bdsk-url-1 = {http://dx.doi.org/10.1348/014466601164939}}

@article{bambergmoser_2007a,
	abstract = {The goal of the present paper is a replication as well as
		  an extension of the Hines et al. [(1986/87). Analysis and
		  synthesis of research on responsible environmental
		  behaviour: A meta-analysis. Journal of Environmental
		  Education, 18, 1-8] meta-analysis on psycho-social
		  determinants of pro-environmental behaviour. Based on
		  information from a total of 57 samples the present
		  meta-analysis finds mean correlations between psycho-social
		  variables and pro-environmental behaviour similar to those
		  reported by Hines et al. In a second step, the matrix of
		  pooled correlations is used for a structural equation
		  modelling (SEM) test of theoretically postulated structural
		  relations between eight determinants of pro-environmental
		  behaviour (so-called Meta-analytic SEM (MASEM)). MASEM
		  results confirm that pro-environmental behavioural
		  intention mediate the impact of all other psycho-social
		  variables on pro-environmental behaviour (27\% explained
		  variance). Results also confirm that besides attitude and
		  behavioural control personal moral norm is a third
		  predictor of pro-environmental behavioural intention (52\%
		  explained variance). The MASEM also indicates that problem
		  awareness is an important but indirect determinant of
		  pro-environmental intention. Its impact seems to be
		  mediated by moral and social norms, guilt and attribution
		  processes.},
